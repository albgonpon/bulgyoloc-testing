{
  "map": {
    "conditions": ["renderer"]
  },
  "about": {
    "title": {
      "express": true,
      "defaultValue": ""
    },
    "splash": {
      "defaultValue": false,
      "express": true,
      "splashButtonPosition": {
        "defaultValue": {
          "position": "top-right",
          "index": 0
        }
      },
      "splashOnStart": {
        "defaultValue": true
      },
      "splashTitle": {
        "express": true,
        "defaultValue": "Splash title"
      },
      "splashContent": {
        "express": true,
        "defaultValue": "Splash content"
      },
      "splashButtonText": {
        "defaultValue": "Okay"
      }
    },
    "customHeader": {
      "defaultValue": false,
      "customHeaderHTML": {
        "defaultValue": ""
      }
    }
  },
  "interactivity": {
    "exploreNavigate": {
      "explore": {
        "home": {
          "defaultValue": true,
          "homePosition": {
            "defaultValue": {
              "position": "top-left",
              "index": 0
            }
          }
        },
        "mapZoom": {
          "defaultValue": true,
          "mapZoomPosition": {
            "defaultValue": {
              "position": "top-left",
              "index": 1
            }
          }
        },
        "infoPanel": {
          "defaultValue": false,
          "infoPanelPosition": {
            "defaultValue": {
              "position": "top-left",
              "index": 2
            }
          }
        },
        "bookmarks": {
          "defaultValue": false,
          "express": false,
          "bookmarksPosition": {
            "defaultValue": {
              "position": "top-right",
              "index": 2
            }
          }
        }
      },
      "navigate": {
        "extentSelector": {
          "defaultValue": false,
          "extentSelectorConfig": {
            "express": false
          }
        }
      }
    },
    "modify": {
      "modify": {
        "basemapToggle": {
          "defaultValue": false,
          "basemapTogglePosition": {
            "defaultValue": {
              "position": "bottom-right",
              "index": 0
            }
          },
          "basemapSelector": {
            "express": false
          }
        },
        "layerList": {
          "defaultValue": false,
          "layerListPosition": {
            "defaultValue": {
              "position": "bottom-right",
              "index": 1
            }
          }
        },
        "timeFilter": {
          "defaultValue": false,
          "express": false,
          "timeFilterPosition": {
            "defaultValue": {
              "position": "bottom-right",
              "index": 2
            }
          },
          "timeFilterConfig": {
            "express": false
          }
        }
      }
    },
    "interactiveLegend": {
      "interactiveLegend": {
        "filterMode": {
          "defaultValue": "featureFilter"
        },
        "muteOpacity": {
          "defaultValue": 30,
          "min": 0,
          "max": 100
        },
        "muteGrayScale": {
          "defaultValue": 70,
          "min": 0,
          "max": 100
        },
        "featureCount": {
          "express": true,
          "defaultValue": false
        },
        "updateExtent": {
          "express": true,
          "defaultValue": false
        }
      }
    },
    "share": {
      "share": {
        "share": {
          "defaultValue": false,
          "express": true
        },
        "sharePosition": {
          "express": true,
          "defaultValue": "top-right"
        }
      },
      "screenshot": {
        "screenshot": {
          "express": false,
          "defaultValue": false,
          "screenshotPosition": {
            "defaultValue": {
              "position": "top-left",
              "index": 3
            }
          },
          "enableLegendOption": {
            "defaultValue": true,
            "includeLegendInScreenshot": {
              "defaultValue": true
            }
          },
          "enablePopupOption": {
            "defaultValue": false,
            "includePopupInScreenshot": {
              "defaultValue": false
            }
          }
        }
      }
    },
    "search": {
      "search": {
        "search": {
          "express": true,
          "defaultValue": true,
          "searchOpenAtStart": {
            "defaultValue": false
          },
          "searchPosition": {
            "defaultValue": {
              "position": "top-right",
              "index": 1
            }
          },
          "searchConfiguration": {
            "express": true
          }
        }
      }
    },
    "customURLParams": {
      "customURLParams": {
        "customURLParamName": {
          "express": false
        },
        "customUrlParam": {
          "express": false,
          "config": {
            "layerSelectionMode": "single",
            "supportedLayerTypes": "*",
            "supportedGeometryTypes": "*",
            "supportsFieldSelection": true,
            "fieldSelectionMode": "single",
            "supportedFieldTypes": "*"
          }
        }
      }
    }
  },
  "themeLayout": {
    "theme": {
      "theme": {
        "theme": {
          "express": true
        },
        "applySharedTheme": {
          "express": true,
          "defaultValue": true
        },
        "interactiveLegendPosition": {
          "express": true,
          "defaultValue": {
            "position": "bottom-left",
            "index": 0
          }
        },
        "customCSS": {
          "defaultValue": ""
        }
      }
    },
    "positionManager": {
      "positionManager": {
        "express": true
      }
    }
  }
}
